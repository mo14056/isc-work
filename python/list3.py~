
#question 3

forward = []

backward = []

values = ["a", "b", "c"]

#for item in values:
    #print item

for item in values:

   forward.append(item)
   backward.insert(0, item)

print "forward is", forward
print "backward is", backward
print forward.reverse() #forward changes the order inside the list but doesnt return them
print forward==backward










